.navbar.navbar-inverse.navbar-fixed-top
	.navbar-inner
		.container
			= render :partial => '/templates/logo'
			.nav-collapse.collapse
				%ul.nav
					%li.dropdown
						= link_to "<i class='icon-music'></i> Music <b class='caret'></b>".html_safe, "#", class: "dropdown-toggle", "data-toggle" => "dropdown"
						%ul.dropdown-menu
							%li
								= link_to "Khmer"
							%li
								= link_to "Korea"
							%li
								= link_to "English"
							%li
								= link_to "Chinese"
					%li.dropdown
						= link_to "<i class='icon-facetime-video'></i> Movie <b class='caret'></b>".html_safe, "#", class: "dropdown-toggle", "data-toggle" => "dropdown"
						%ul.dropdown-menu
							%li
								= link_to "Hollywood"
							%li
								= link_to "Khmer"
							%li
								= link_to "Korea"
							%li
								= link_to "Chinese"
		= yield :search